/*** QLineEdit ***/
QLineEdit{
	font: 10pt &quot;Montserrat&quot;;
	color: rgb(0, 1, 146);
	padding: 10px;
	background-color: rgb(186, 209, 248);
}


/**** QSpinBox ****/
QSpinBox {
    background-color: rgb(186, 209, 248); /* Couleur d'arrière-plan */
    color: rgb(0, 1, 146);                /* Couleur du texte */
    border: 1px outset gray;     /* Bordure */
	box-shadow: outset 5px 5px 10px gray;
    border-radius: 1px;                   /* Coins arrondis */
    padding: 10px;                         /* Espacement interne */
	border-bottom: 1px outset gray;
}

QSpinBox::up-button, QSpinBox::down-button {
   /*background-color: transparent;        /* Rendre les boutons transparents pour voir la couleur de fond */
    border: none;                         /* Supprime les bordures des boutons */
    width: 20px;                          /* Largeur des boutons */
}

QSpinBox::up-arrow, QSpinBox::down-arrow {
    width: 10px;                          /* Taille des flèches */
    height: 10px;
    background: rgb(0, 1, 146);                     /* Supprime les fonds des flèches */
    color: white;                /* Couleur des flèches */
}

QSpinBox:focus {
    border: 1px solid rgb(65, 173, 255);   /* Bordure différente en cas de focus */
}

/******************************/



QRadioButton{
	color: rgb(51, 51, 51);
	font-size: 14px;
}


/**** QComboBox ****/
QComboBox {
	font: 10pt &quot;Montserrat&quot;;
	/*background-color: rgb(186, 209, 248);*/
	color: rgb(0, 1, 146);
	padding: 10px;
	/*box-shadow: none;
	border: none;*/
}

QComboBox::drop-down {
    border: none; /* Supprime la bordure du bouton de la flèche */
	subcontrol-origin: padding;
    subcontrol-position: top right;
}

QComboBox QAbstractItemView {
    background-color: rgb(186, 209, 248); /* Couleur de fond du menu déroulant */
    color: rgb(0, 1, 146);               /* Couleur du texte */
  /* border: 1px solid rgb(150, 150, 150); /* Bordure pour alignement avec le parent */
	font: 10pt &quot;Montserrat&quot;;
   /* margin: 0 20px;                         /* Supprime les débordements */
    padding: 0 10px;
    outline: none;                    /* Supprime l'effet de focus */
	border: none; 
	box-shadow: none;
}

QComboBox QAbstractItemView::item {
    padding: 10px;                       /* Ajoute un espacement entre les éléments */
}

QComboBox QAbstractItemView::item:selected {
    background-color: rgb(150, 180, 220); /* Couleur pour l'élément sélectionné */
    color: white;                        /* Couleur du texte pour l'élément sélectionné */
}</string>
